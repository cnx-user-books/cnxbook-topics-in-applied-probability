<m:mtr>
<m:mtd>
<m:mrow>
                <m:mi>E</m:mi>
                <m:mrow>
                  <m:mo>[</m:mo>
                  <m:msubsup>
                    <m:mi>X</m:mi>
                    <m:mi>p</m:mi>
                    <m:mn>2</m:mn>
                  </m:msubsup>
                  <m:mo>]</m:mo>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:munderover>
                  <m:mo>∑</m:mo>
                  <m:mrow>
                    <m:mi>k</m:mi>
