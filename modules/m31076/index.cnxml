                </m:msub>
                <m:mo>-</m:mo>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>p</m:mi>
                </m:msub>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>]</m:mo>
              <m:mo>=</m:mo>
              <m:mn>0</m:mn>
</m:mrow>

</m:mtd>
<m:mtd>
<m:mrow>
              <m:mo>∀</m:mo>
</m:mrow>
</m:mtd>
<m:mtd>
<m:mrow>
              <m:mi>m</m:mi>
              <m:mo>&lt;</m:mo>
              <m:mi>n</m:mi>
              <m:mo>≤</m:mo>
              <m:mi>p</m:mi>
              <m:mo>&lt;</m:mo>
              <m:mi>q</m:mi>
              <m:mo>∈</m:mo>
              <m:mi mathvariant="bold">N</m:mi>
</m:mrow>     
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mrow>
              <m:mi>E</m:mi>
              <m:mrow>
                <m:mo>[</m:mo>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>p</m:mi>
                </m:msub>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>q</m:mi>
                </m:msub>
                <m:mo>]</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mi>E</m:mi>
              <m:mrow>
                <m:mo>[</m:mo>
                <m:msubsup>
                  <m:mi>X</m:mi>
                  <m:mrow>
                    <m:mi>p</m:mi>
                    <m:mo>∧</m:mo>
                    <m:mi>q</m:mi>
                  </m:mrow>
                  <m:mn>2</m:mn>
                </m:msubsup>
                <m:mo>]</m:mo>
              </m:mrow>
</m:mrow>
</m:mtd>
<m:mtd>
<m:mrow>
              <m:mo>∀</m:mo>
</m:mrow>
</m:mtd>
<m:mtd>
<m:mrow>
              <m:mi>p</m:mi>
