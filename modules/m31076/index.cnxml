              <m:mi>p</m:mi>
              <m:mo>&lt;</m:mo>
              <m:mi>q</m:mi>
              <m:mo>∈</m:mo>
              <m:mi mathvariant="bold">N</m:mi>
</m:mrow>
</m:mtd>
</m:mtr>

<m:mtr>
<m:mtd>
<m:mrow>           
              <m:mi>E</m:mi>
              <m:mo>[</m:mo>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>n</m:mi>
                </m:msub>
                <m:mo>-</m:mo>
                <m:msub>
                  <m:mi>X</m:mi>
                  <m:mi>m</m:mi>
                </m:msub>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:msub>
                  <m:mi>X</m:mi>
