</m:mrow>
</m:mtd>
</m:mtr>
          
<m:mtr>
<m:mtd>
<m:mrow>         
              <m:mi>E</m:mi>
              <m:mrow>
                <m:mo>[</m:mo>
                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:msub>
                      <m:mi>X</m:mi>
                      <m:mi>q</m:mi>
                    </m:msub>
                    <m:mo>-</m:mo>
                    <m:msub>
                      <m:mi>X</m:mi>
                      <m:mi>p</m:mi>
                    </m:msub>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mo>]</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mi>E</m:mi>
              <m:mrow>
                <m:mo>[</m:mo>
                <m:msubsup>
                  <m:mi>X</m:mi>
                  <m:mi>q</m:mi>
                  <m:mn>2</m:mn>
                </m:msubsup>
                <m:mo>]</m:mo>
              </m:mrow>
              <m:mo>-</m:mo>
              <m:mi>E</m:mi>
              <m:mrow>
                <m:mo>[</m:mo>
                <m:msubsup>
                  <m:mi>X</m:mi>
                  <m:mi>p</m:mi>
                  <m:mn>2</m:mn>
                </m:msubsup>
                <m:mo>]</m:mo>
              </m:mrow>
              <m:mo>≥</m:mo>
