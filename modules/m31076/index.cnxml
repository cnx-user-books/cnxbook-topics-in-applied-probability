                    <m:mn>0</m:mn>
                  </m:mrow>
                  <m:mi>p</m:mi>
                </m:munderover>
                <m:mi>E</m:mi>
                <m:mrow>
                  <m:mo>[</m:mo>
                  <m:msubsup>
                    <m:mi>Y</m:mi>
